
<%- contentFor('body') %>

<section class="page-content">
    <section class="panel">
        <div class="panel-heading">
            <h3>
               Networks
            </h3>
        </div>

      <div class="panel-body">
        <div class="layout-grid-example">
         <div class="container-fluid">
            <div class="row">
            <% for(var i=1; i <=15; i++) { %>
                        <div class="col-md-3"  style="text-align:center;cursor:pointer"  ng-click="networkDetails()">
                         <span><%=i%></span>

                        </div>
                        <div class="col-md-3  "  style="text-align:center; cursor:pointer" ng-click="networkDetails()">
                         <span><%=i+15%></span>
                         
                        </div>
                        <div class="col-md-3   "  style="text-align:center; cursor:pointer" ng-click="networkDetails()">
                         <span><%=i+30%></span>
                         
                        </div>
                        <div class="col-md-3 "  style="text-align:center; cursor:pointer" ng-click="networkDetails()">
                         <span><%=i+45%></span>
                         
                        </div>
                 <% } %>
             </div>
           </div>
          </div>
       </div>    
    </section>
</section>

<%- contentFor('page_script') %>
